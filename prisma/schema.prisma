generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Movie {
  id          String     @id @default(uuid())
  title       String     @unique
  description String
  url         String
  categories  Category[]

  @@map("Movies")
}

model Category {
  id     String  @id @default(uuid())
  name   String  @unique
  color  String
  videos Movie[]

  @@map("Categories")
}
